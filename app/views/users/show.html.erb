<% if current_user && current_user.admin? %>
<p id="notice"><%= notice %></p>

<p>
  <strong>Netid:</strong>
  <%= @user.netid %>
</p>

<p>
  <strong>Admin:</strong>
  <%= @user.admin %>
</p>

<%= link_to 'Edit', edit_user_path(@user) %> |
<%= link_to 'Back', users_path %>
<% else %>
<div class="alert alert-error">
  <p> You aren't authorized! </p>
</div>
<% end %>
  
  
